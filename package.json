{
  "name": "password-genie",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "homepage": "git@github.com:Sharlaan/password-genie",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "unit": "vue-cli-service test:unit",
    "e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open",
    "deploy": "node ./node_modules/vue-gh-pages/index.js --branch gh-pages"
  },
  "dependencies": {
    "register-service-worker": "^1.6.2",
    "unsplash-source-es6": "^1.2.1",
    "vue": "^2.6.8",
    "vue-class-component": "^7.0.1",
    "vue-clipboards": "^1.2.4",
    "vue-property-decorator": "^8.0.0",
    "vuetify": "^1.5.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-e2e-cypress": "^3.4.1",
    "@vue/cli-plugin-pwa": "^3.4.1",
    "@vue/cli-plugin-typescript": "^3.4.1",
    "@vue/cli-plugin-unit-jest": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333",
    "vue-gh-pages": "^1.19.0",
    "vue-template-compiler": "^2.6.8"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 3%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.(spec|test).(ts|tsx|js)|**/__tests__/*.(ts|tsx|js))"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
